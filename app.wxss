/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.imgList{
  width:100%;
  height: auto;
  display: flex;
  flex-direction: column;
}
.imgList image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}